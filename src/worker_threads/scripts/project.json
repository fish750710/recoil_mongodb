{
  "name": "aifa",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/aifa/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project aifa --web",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/aifa/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "all-build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node ./apps/aifa/scripts/all-build.mjs"
      }
    }
  }
}
